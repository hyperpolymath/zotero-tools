{
  "name": "zotero-nsai",
  "version": "0.1.0-alpha",
  "description": "NSAI: Neurosymbolic validation and preparation for Zotero research data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "zotero",
    "validation",
    "neurosymbolic",
    "research",
    "citations",
    "wittgenstein",
    "tractatus",
    "bibliography",
    "formal-verification",
    "type-safety",
    "offline-first",
    "privacy-first",
    "accessibility",
    "rsr-compliant"
  ],
  "author": "Hyperpolymath",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hyperpolymath/zotero-nsai.git"
  },
  "bugs": {
    "url": "https://github.com/Hyperpolymath/zotero-nsai/issues"
  },
  "homepage": "https://github.com/Hyperpolymath/zotero-nsai#readme",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/Hyperpolymath"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "zod": "^4.2.1"
  },
  "rsr": {
    "compliance": "silver",
    "version": "1.0.0",
    "verified": "2024-11-22"
  },
  "security": {
    "policy": "https://github.com/Hyperpolymath/zotero-nsai/blob/main/SECURITY.md",
    "contact": "https://github.com/Hyperpolymath/zotero-nsai/security/advisories/new"
  },
  "tpcf": {
    "perimeter": 3,
    "description": "Community Sandbox - Open Contribution"
  }
}
